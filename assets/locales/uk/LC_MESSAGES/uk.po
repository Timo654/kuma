#: gui_kuma.py:1294
msgid "<b>Delete</b> - Removes currently selected/edited note."
msgstr "<b>Delete</b> - Видалити вибрану ноту."

#: gui_kuma.py:1295
msgid "<b>End</b> - Jump to the last note."
msgstr "<b>End</b> - Перейти к останньої ноти."

#: gui_kuma.py:1296
msgid "<b>Left Ctrl + S</b> - Save."
msgstr "<b>Left Ctrl + S</b> - Зберiгти."

#: gui_kuma.py:1297
msgid "<b>Left Ctrl + Z</b> - Undo."
msgstr "<b>Left Ctrl + Z</b> -  Скасувати."

#: gui_kuma.py:1298
msgid "<b>Left Ctrl + F</b> - Select a note."
msgstr "<b>Left Ctrl + F</b> - Оберiть ноту."

#: gui_kuma.py:1299
msgid "<b>Left Ctrl + C</b> - Copy selected notes."
msgstr "<b>Left Ctrl + C</b> - Копiювати обранi ноти."

#: gui_kuma.py:1300
msgid "<b>Left Ctrl + V</b> - Paste copied notes (Vertical position does not change)."
msgstr "<b>Left Ctrl + V</b> - Вставити скопiйованi ноти (Вертикальна позицiя не змiнюється)."

#: gui_kuma.py:1301
msgid "<b>Left Ctrl + Left Shift + V</b> - Paste copied notes (Vertical position changes)."

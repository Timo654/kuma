msgstr ""

#: gui_kuma.py:1290
msgid "<b>Left click</b> - Place and pick up notes."
msgstr ""

#: gui_kuma.py:1291
msgid "<b>Right click</b> - Change held note type."
msgstr ""

#: gui_kuma.py:1292
msgid "<b>E</b> - Note edit mode. You can accurately change note timings, position, type and more. Pressing E again saves the note."
msgstr ""

#: gui_kuma.py:1293
msgid "<b>Arrow keys, Page Up, Page Down</b> - Move the scrollbar."
msgstr ""

#: gui_kuma.py:1294
msgid "<b>Delete</b> - Removes currently selected/edited note."
msgstr ""

#: gui_kuma.py:1295
msgid "<b>End</b> - Jump to the last note."
msgstr ""

#: gui_kuma.py:1296
msgid "<b>Left Ctrl + S</b> - Save."
